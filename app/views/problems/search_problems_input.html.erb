<div class="container">
  <div class="row">
    <div class="col-md-3">
      <%= form_tag problems_path, method: :get do %>
        <%= text_field_tag :search, params[:search] %>
        <%= button_tag "Search", name: nil %>
      <% end %>
    </div>
  </div>
  <div class="row">
    <div class="col-md-3"
      <p><%= link_to "Advanced Search", new_search_problem_path %></p>
      <div id="problems">
      </div>
    </div>
  </div>
</div>
