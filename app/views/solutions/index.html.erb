<h1>Listing solutions</h1>

<table>
  <thead>
    <tr>
      <th>Problem</th>
      <th>Project</th>
      <th>Solution abstract</th>
      <th>Published at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solutions.each do |solution| %>
      <tr>
        <td><%= solution.problem_id %></td>
        <td><%= solution.project_id %></td>
        <td><%= solution.solution_abstract %></td>
        <td><%= solution.published_at %></td>
        <td><%= link_to 'Show', solution %></td>
        <td><%= link_to 'Edit', edit_solution_path(solution) %></td>
        <td><%= link_to 'Destroy', solution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Solution', new_solution_path %>
