

	<% if @objparent.nil? %>
	<% @objparent = @user %>
	<% end %>


	<% @self_profile_headers = @objparent.profiles.where("made_public_at IS NOT NULL or user_id = " + current_user.id.to_s) %>

	<% @self_profile_headers.group(:name).each do |p| %>


          <div class="panel panel-default">
          <div class="panel-heading"> <a></a><h4>
			<%= p.name %>  </h4></div>
 <div class="panel-body">
              <div class="list-group">
			<% @self_profile_headers.each do |b| %>

			<% if b.name == p.name %>


					<% if b.bio =~ /http/ or b.bio =~ /www/ or b.bio =~/:/ %>

<a class ='list-group-item'>
					<%= link_to b.bio[0..25], b.bio, :class =>'list-group-item'  %>

					<%= link_to 'Link', b %> </a>

					<% else %>

					<%= link_to b.bio[0..25], b, :class =>'list-group-item' %>

					<% end %>



					<small><br>
						<% if b.made_public_at.nil? %>
							<%= 'Private' %>
						<% else %>
							<%= b.made_public_at.to_date  %>
						<% end %>
					</small>

					</td>

				</tr>

			<% end %>
			<% end %>
              </div>
            </div>
	          </div>

	<% end %>


