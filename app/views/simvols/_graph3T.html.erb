
<% @graph = [] %>

<% @wordlist.each do |wl| %>
	<% Simvol.find_or_create_by(name: wl).tags.each do |wlt|  #Find by name or make new record, AKA BEST THING EVER!!!!%>


		<% wlt.tags.each do |t| %>

			<% @graph << t %>

			<% Simvol.find(t).tags.each do |t2|  %>

				<% @graph << t2   %>

					<% Simvol.find(t2).tags.each do |t3| %>

						<% @graph << t3 %>

							<% Simvol.find(t3).tags.each do |t4| %>

								<% @graph << t4 %>

							<% end #t4 %>

				<% end #t3 %>
			<% end #t2 %>
		<% end #t %>
	<% end #wlt %>

<% end #wl %>


